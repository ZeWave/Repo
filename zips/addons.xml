	</requires>  
	<extension point="xbmc.addon.repository" name="The Wave repo">
		<!-- Do not forget to bump add-on version when changing paths to force a repo refresh -->
        <dir>
            <info compressed="false">https://raw.githubusercontent.com/Zewave/Repo/master/zips/addons.xml</info>
            <checksum>https://raw.githubusercontent.com/Zewave/Repo/master/zips/addons.xml.md5</checksum>
            <datadir zip="true">https://raw.githubusercontent.com/Zewave/Repo/master/zips/</datadir>
		</dir>
	</extension>
	<extension point="xbmc.addon.metadata">
