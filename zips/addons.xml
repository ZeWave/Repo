<addon id="plugin.program.waveHeb" name="[COLOR yellow]The Wave Hebrew Wizard[/COLOR]" version="0.0.2" provider-name="The Wave Team">
    <requires>
        <import addon="xbmc.python" version="3.0.0" />
        <import addon="script.module.requests" />
        <import addon="script.module.six" />
    </requires>    
    <extension point="xbmc.python.pluginsource" library="default.py">
        <provides>executable</provides>
    </extension>    
    <extension point="xbmc.service" library="startup.py" start="startup" />    
    <extension point="xbmc.addon.metadata">
        <summary lang="en">[COLOR blue]The Wave Hebrew Wizard[/COLOR]</summary>
        <description lang="en">[COLOR blue]The Wave Hebrew Wizard[/COLOR]</description>
        <platform>all</platform>
        <reuselanguageinvoker>false</reuselanguageinvoker>
        <source>https://www.github.com/a4k-openproject/plugin.program.openwizard/</source>
        <assets>
            <icon>icon.png</icon>
            <fanart>fanart.jpg</fanart>
        </assets>
    </extension>
</addon>

